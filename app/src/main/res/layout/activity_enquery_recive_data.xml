<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_margin="10dp"
    android:layout_height="wrap_content"
    tools:context=".EnqueryReciveData">


    <androidx.cardview.widget.CardView

        android:layout_marginTop="40dp"
        app:cardElevation="10dp"
        app:cardCornerRadius="20dp"
        android:background="@color/purple_700"
        app:cardMaxElevation="12dp"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/text1"
        android:gravity="center"
        android:layout_gravity="center"
        android:textColor="#333333"
        android:textStyle="bold"
        android:fontFamily="sans-serif"
        android:text="Enquery Details"
        android:layout_margin="20sp"
        android:layout_marginTop="15dp"
        android:textSize="35sp"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"/>

    <LinearLayout
        android:id="@+id/lin2"
        android:layout_below="@id/text1"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="#333333"
            android:textStyle="bold"
            android:text="First Name "
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:textSize="20sp"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"/>



    <TextView
       android:layout_gravity="center"
        android:text="first name"
        android:layout_margin="20sp"
        android:layout_marginTop="30dp"
        android:textSize="20sp"
        android:id="@+id/FName_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

        </LinearLayout>


    <LinearLayout
        android:layout_below="@+id/lin2"
        android:id="@+id/lin3"
        android:layout_marginLeft="30dp"

        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="#333333"
            android:textStyle="bold"
            android:text="LastName"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:textSize="20sp"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"/>


    <TextView
        android:layout_gravity="center"

        android:id="@+id/LName_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginTop="30dp"
        android:text="Last Name "
        android:textSize="20sp"
        tools:ignore="DuplicateIds" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/line4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_below="@id/lin3"
        android:layout_marginLeft="30dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:text="Selphone No"
            android:textColor="#333333"
            android:textSize="20sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/phone_text"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:text="phone no"
            android:textSize="20sp"
            tools:ignore="DuplicateIds" />
    </LinearLayout>




    <LinearLayout
        android:id="@+id/gen_text"
        android:layout_below="@+id/line4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginLeft="30dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:text="E-mail Id "
            android:textColor="#333333"
            android:textSize="20sp"

            android:textStyle="bold" />

        <TextView

            android:id="@+id/Email_text"

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:text="Email "
            android:textSize="20sp"
            tools:ignore="DuplicateIds" />
    </LinearLayout>

    <LinearLayout
        android:layout_below="@id/gen_text"
        android:id="@+id/line5"
        android:layout_marginLeft="30dp"

        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="#333333"
            android:textStyle="bold"
            android:text="Gender"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:textSize="20sp"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center"
            android:id="@+id/gender_text"
           android:layout_width="match_parent"
          android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginTop="30dp"
        android:text="gender "
        android:textSize="20sp"
        />
    </LinearLayout>


    <LinearLayout
        android:layout_below="@+id/line5"
        android:layout_marginLeft="30dp"
        android:id="@+id/line6"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="#333333"
            android:textStyle="bold"
            android:text="Course"
            android:layout_margin="20sp"
            android:layout_marginTop="30dp"
            android:textSize="20sp"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center"
        android:id="@+id/Course_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20sp"
        android:layout_marginTop="30dp"
        android:text="Course "
        android:textSize="20sp"
        tools:ignore="DuplicateIds" />

    </LinearLayout>

</RelativeLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>